{
  "name": "patze-control",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "packageManager": "pnpm@9.0.0",
  "scripts": {
    "build": "pnpm -r build",
    "build:api-server": "pnpm --filter @patze/telemetry-core build && pnpm --filter @patze/api-server build",
    "build:desktop": "pnpm --filter @patze/desktop build",
    "build:openclaw-bridge": "pnpm --filter @patze/openclaw-bridge build",
    "dev:api-server": "pnpm --filter @patze/telemetry-core build && pnpm --filter @patze/api-server dev",
    "dev:desktop": "pnpm --filter @patze/desktop dev",
    "dev:desktop:tauri": "pnpm --filter @patze/desktop tauri dev",
    "dev:openclaw-bridge": "pnpm --filter @patze/openclaw-bridge dev",
    "lint": "pnpm -r lint",
    "format": "prettier --check .",
    "format:write": "prettier --write .",
    "typecheck": "pnpm -r typecheck"
  },
  "devDependencies": {
    "@eslint/js": "^9.20.0",
    "eslint": "^9.20.1",
    "eslint-config-prettier": "^10.0.1",
    "prettier": "^3.5.1",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.24.1"
  }
}

{
  "name": "@patze/openclaw-bridge",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "openclaw-bridge": "dist/index.js"
  },
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "build:bundle": "esbuild src/index.ts --bundle --platform=node --format=esm --outfile=dist/bridge-bundle.mjs",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "lint": "eslint \"src/**/*.ts\"",
    "dev": "tsx watch src/index.ts",
    "start": "node dist/index.js"
  },
  "dependencies": {
    "@patze/telemetry-core": "workspace:*"
  },
  "devDependencies": {
    "@types/node": "^25.3.2",
    "esbuild": "^0.27.3",
    "tsx": "^4.19.2"
  }
}
